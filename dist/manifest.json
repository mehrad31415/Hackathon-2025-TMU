{
  "name": "AI Image Blur",
  "description": "Automatically blurs any image classified as stuff from the blocked list.",
  "version": "0.2.0",

  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab"
  ],

  "host_permissions": [
    "<all_urls>"
  ],

  "background": {
    "service_worker": "src/service_worker.js"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "manifest_version": 3,
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
   "128": "images/get_started128.png"
  }
}
